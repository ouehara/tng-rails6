<p id="notice"><%= notice %></p>

<h1>Top Page Sections Elements</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @top_page_sections_elements.each do |top_page_sections_element| %>
      <tr>
        <td><%= top_page_sections_element.title %></td>
        <td><%= top_page_sections_element.link %></td>
        <td><%= link_to 'Show', top_page_sections_element %></td>
        <td><%= link_to 'Edit', edit_top_page_sections_element_path(top_page_sections_element) %></td>
        <td><%= link_to 'Destroy', top_page_sections_element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Top Page Sections Element', new_admin_top_page_sections_element_path %>
