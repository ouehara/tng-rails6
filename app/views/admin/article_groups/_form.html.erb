<%= form_for [:admin,@article_group] do |f|%>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control", placeholder: "Title", autocomplete: "off" %>
  </div>
  <div class="form-group">
   <%= f.select :parent_id, @categories, :include_blank => true %>
  </div>
    <%= f.submit %>
<%end%>